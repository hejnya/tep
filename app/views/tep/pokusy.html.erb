<%#<h1>Tep Pikomatu MFF UK</h1> %>

<% @no_sidebar  = true %>
<% @tep_index = true %>

<div class="bouda">
  <div class="sidebar responsive x-menu-min" id="sidebar">
    <ul class="nav nav-list">
      <%= menu_li '/onas', 'O Pikomatu', ico: 'fa-square', cls: 'open active'  do %>
        <ul class="submenu">
          <%= menu_lii wiki_web_path('main'),               'Úvod' %>
          <%= menu_lii wiki_web_path('index'),               'Hodnoty a cíle' %>
          <%= menu_lii wiki_web_path('onas/organizatori'),   'Organizátoři' %>
          <%= menu_lii wiki_web_path('onas/onas'),           'Kontakty'  %>
          <%= menu_lii wiki_web_path('onas/historie'),        'Historie'   %>
        </ul>
      <% end %>
    </ul>
  </div>
  <div class="sidebar responsive x-menu-min" id="sidebar">
    <ul class="nav nav-list">
      <%= menu_li '/onas', 'Zadání', ico: 'fa-edit', cls: 'open active' do %>
        <ul class="submenu">
          <%= menu_lii wiki_web_path('archiv/rocnik29/zad5'),    'Aktuální série' %>
          <%= menu_lii wiki_web_path('archiv'),                  'Archiv příkladů' %>
          <%= menu_lii wiki_web_path('onas/pravidla'),           'Pravidla'  %>
          <%= menu_lii wiki_web_path('onas/faq'),                'Otázky a odpovědi'   %>
          <%= menu_lii wiki_web_path('todo'),      '_'  %>
        </ul>
      <% end %>
    </ul>
  </div>
  <div class="sidebar responsive x-menu-min" id="sidebar">
    <ul class="nav nav-list">
      <%= menu_li '/onas', 'Akce', ico: 'fa-search', cls: 'open active' do %>
        <ul class="submenu">
          <%= menu_lii wiki_web_path('tabor'),     'Tábory' %>
          <%= menu_lii wiki_web_path('soustredeni'),   'Soustředění' %>
          <%= menu_lii wiki_web_path('todo'),      'Ostatní'  %>
          <%= menu_lii wiki_web_path('todo'),      '_'  %>
          <%= menu_lii wiki_web_path('todo'),      '_'  %>
        </ul>
      <% end %>
    </ul>
  </div>
  <div class="sidebar responsive x-menu-min" id="sidebar">
    <ul class="nav nav-list">
      <%= menu_li '/onas', 'Přihláška', ico: 'fa-book', cls: 'open active' do %>
        <ul class="submenu">
          <%= menu_lii :new_user_session,        'Přihlásit' %>
          <%= menu_lii :new_user_session,   'Založit účet' %>
          <%= menu_lii wiki_web_path('/tabor/2015/prihlaska'),  'Přáhláška na tábor'  %>
          <%= menu_lii wiki_web_path('todo'),      '_'  %>
          <%= menu_lii wiki_web_path('todo'),      '_'  %>
        </ul>
      <% end %>
      <% if user_signed_in?  %>
        <%= menu_li '#', 'Uživatel', ico: 'fa-user', cls: 'open active' do %>
          <ul class="submenu">
            <%= menu_lii :sosna_solutions_user,   'Moje řešení' %>
            <%= menu_lii :edit_user_registration, 'Změna hesla' %>
            <%= menu_lii :faq,                    'Nápověda'   %>
          </ul>
        <% end %>
      <% else %>
      <% end %>
      <% if current_user && current_user.has_role?(:org) %>
        <%= menu_li '#', 'Org', ico: 'fa-flag' do %>
          <ul class="submenu">
            <%= menu_lii :sosna_solvers,       'Řešitelé' %>
            <%= menu_lii :sosna_solutions_org, 'Řešení řešitelů' %>
            <%= menu_lii :sosna_problems,      'Úlohy' %>
            <%= menu_lii :sosna_schools,       'Školy' %>
            <%= menu_lii :wiki_wiki,           'Wiki' %>
            <%#= menu_lii :wiki_web,            'Web' %>
            <%= menu_lii :wiki_web_edit,       'Web Edit'%>
          </ul>
        <% end %>
      <% end %>
      <% if current_user && current_user.has_role?(:admin) %>
        <%= menu_li '#', 'Admin', ico: 'fa-eye' do %>
          <ul class="submenu">
            <%= menu_lii :users_list,       'Uživatelé' %>
            <%= menu_lii :sosna_configs,   'Konfigurace' %>
          </ul>
        <% end %>
      <% end %>
    </ul>
  </div> <%# sidebar %>

</div>


